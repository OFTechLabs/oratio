{
  "name": "@oratio/oratio",
  "version": "0.1.0",
  "description": "Emergent natural language interfacing",
  "author": "Jelle Verstegen",
  "license": "MIT",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "typings": "dist/main.d.ts",
  "jam": {
    "main": "./dist/main.browser.js"
  },
  "browser": {
    "dist/main.js": "./dist/main.browser.js"
  },
  "scripts": {
    "build": "webpack || echo not ok",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prerelease": "npm run build && npm test",
    "release": "standard-version && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jelleverstegen/oratio.git"
  },
  "keywords": [
    "nlp conversational"
  ],
  "bugs": {
    "url": "https://github.com/jelleverstegen/oratio/issues"
  },
  "homepage": "https://github.com/jelleverstegen/oratio",
  "devDependencies": {
    "@types/chai": "4.0.3",
    "@types/jest": "20.0.7",
    "@types/node": "8.0.22",
    "awesome-typescript-loader": "3.2.3",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "browserify": "14.4.0",
    "chai": "4.1.1",
    "delete-empty": "1.0.1",
    "dts-bundle": "0.7.3",
    "jest": "20.0.4",
    "json-loader": "0.5.7",
    "standard-version": "4.2.0",
    "tslint": "5.6.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.4.2",
    "typescript-babel-jest": "1.0.5",
    "webpack": "3.5.4",
    "webpack-node-externals": "1.6.0"
  },
  "dependencies": {
    "@types/mathjs": "0.0.34",
    "mathjs": "3.16.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest"
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
